{
  "name": "memori-js",
  "version": "1.0.59",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "test": "bun test",
    "build": "bun build ./src/index.ts --outdir ./dist --target node --external sqlite3 && tsc",
    "start": "bun run dist/index.js"
  },
  "type": "module",
  "description": "SQL-Native AI Memory Fabric for JavaScript/TypeScript, powered by sqlite-vec and Google GenAI.",
  "license": "MIT",
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/shambhav15/memori-js.git"
  },
  "keywords": [
    "ai",
    "memory",
    "vector-database",
    "sqlite",
    "embeddings",
    "rag",
    "agent"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@google/genai": "^1.32.0",
    "@types/pg": "^8.16.0",
    "@xenova/transformers": "^2.17.2",
    "arktype": "^2.1.28",
    "better-sqlite3": "^12.5.0",
    "elysia": "latest",
    "openai": "^6.10.0",
    "pg": "^8.16.3",
    "sharp": "^0.34.5",
    "sqlite-vec": "^0.1.7-alpha.2",
    "sqlite3": "^5.1.7"
  },
  "optionalDependencies": {
    "sqlite-vec-darwin-arm64": "^0.1.7-alpha.2",
    "sqlite-vec-darwin-x64": "^0.1.7-alpha.2",
    "sqlite-vec-linux-x64-gnu": "^0.1.7-alpha.2",
    "sqlite-vec-win32-x64": "^0.1.7-alpha.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.0.0",
    "@types/sqlite3": "^5.1.0",
    "bun-types": "latest",
    "typescript": "^5.9.3"
  }
}
